{
  "expo": {
    "name": "roadrate-mobile",
    "slug": "roadrate-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "roadrate-mobile",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.roadrate.roadrate-mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.roadrate.roadrate-mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "auth0Domain": "@REACT_APP_AUTH0_DOMAIN",
      "auth0ClientId": "@REACT_APP_AUTH0_CLIENT_ID"
    }
  }
}
